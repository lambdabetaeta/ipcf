@article{Davies1996a,
abstract = {The Curry-Howard isomorphism identifies proofs with typed &lambda;-calculus terms, and correspondingly identifies propositions with types. We show how this isomorphism can be extended to relate constructive temporal logic with binding-time analysis. In particular we show how to extend the Curry-Howard isomorphism to include the O (&ldquo;next&rdquo;) operator from linear-time temporal logic. This yields the simply typed &lambda;O-calculus which we prove to be equivalent to a multi-level binding-time analysis like those used in partial evaluation for functional programming languages. Further, we prove that normalization in &lambda;O can be done in an order corresponding to the times in the logic, which explains why &lambda;O is relevant to partial evaluation. We then extend &lambda;O to a small functional language, Mini-MLO, and give an operational semantics for it. Finally, we prove that this operational semantics correctly reflects the binding-times in the language, a theorem which is the functional programming analog of time-ordered normalization},
author = {Davies, Rowan},
doi = {10.1109/LICS.1996.561317},
file = {:media/alex/STORAGE/Mendeley/Davies/Davies - 1996 - A temporal-logic approach to binding-time analysis.pdf:pdf},
