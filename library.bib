@article{Kleene1980,
abstract = {In my June 13, 1977 Oslo address (Generalized recursion theory 11, 1978) I overcame the limitations in my 1959, 1963 theory on substitution of ??-functionals and on the use of the first recursion theorem by introducing a new list of schemata, with computation rules that do not require values of parts to be computed unless and until they are needed. Computation thereby became starkly formal. The present paper takes steps toward developing a certain semantics to flesh out the formal bones of those computations. The old types 0,1,2 are represented within new types {black small square} where type 0 = {u, 0, 1,2,...} (u = "undefined" or "unknown"), and the higher types are composed of the "unimonotone" partial one-place functions from the preceding type into {0,1,2,...}. These are the functions which are monotone and can each be embodied in an oracle who, e.g. at type 2, has been programmed by Apollo to respond (if she does), to any question "{black small square}2({black small square}1)?" we ask, on the basis of information about {black small square}1 which she obtains by asking in turn questions of an oracle embodying {black small square}1 and observing her responses. ?? 1980 North-Holland Publishing Company.},
author = {Kleene, Stephen C.},
doi = {10.1016/S0049-237X(08)71251-5},
file = {:media/alex/STORAGE/Mendeley/Kleene/Kleene - 1980 - Recursive Functionals and Quantifiers of Finite Types Revisited II.pdf:pdf},
issn = {0049237X},
journal = {Studies in Logic and the Foundations of Mathematics},
number = {C},
pages = {1--29},
title = {{Recursive Functionals and Quantifiers of Finite Types Revisited II}},
volume = {101},
year = {1980}
}
